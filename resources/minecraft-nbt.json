{
    "base": {
        "block": {
            "CanPlaceOn": {
                "type": "string array",
                "values": "#blocks"
            },
            "BlockEntityTag": {
                "type": "compound"
            }
        },
        "entity": {
            "Age": "int",
            "Passengers": {
                "type": "compound array",
                "format": {
                    "id": {
                        "type": "string",
                        "values": "#entities"
                    }
                }
            },
            "Fire": "int",      
            "Motion": {
                "type": "double array",
                "length": 3
            },
            "NoGravity": "boolean"
        },
        "living_entity": {
            "parent": "entity",
            "ActiveEffects": {
                "type": "compound array",
                "format": {
                    "Id": "int",
                    "Duration": "int",
                    "Amplifier": "byte",
                    "Ambient": "byte"                    
                }
            },
            "NoAI": "boolean",
            "Silent": "boolean",
            "Invulnerable": "boolean",
            "Attributes": {
    
            },            
            "ArmorItems": {
                "type": "compound array",
                "format": {
                    "id": {
                        "type": "string",
                        "values": "#items"
                    },
                    "Count": "byte"
                }
            },
            "HandItems": {
                "type": "compound array",
                "format": {
                    "id": {
                        "type": "string",
                        "values": "#items"
                    },
                    "Count": "byte"
                }
            },
            "DropChances": {
                "type": "float array",
                "length": 5
            }
        }
    },
    "group": {
        "entity": {
            "#fireball-like": [
                "fireball",
                "dragon_fireball",
                "small_fireball",
                "wither_skull"
            ]
        },
        "block": {

        }
    },
    "blocks": {

    },
    "entities": {
        "#fireball-like": {
            "parent": "entity",
            "ExplosionPower": "int",
            "direction": {
                "type": "double array",
                "length": 3
            },
            "power": {
                "type": "double array",
                "length": 3
            }
        },
        "armor_stand": {
            "parent": {
                "name": "living_entity",
                "exclude": [
                    "NoAI",
                    "Silent",
                    "DropChance",
                    "Attributes",
                    "ActiveEffects"
                ]
            },
            "ShowArms": "boolean",
            "NoBasePlate": "boolean",
            "Small": "boolean",
            "Rotation": {
                "type": "float array",
                "length": 2
            },
            "Marker": "boolean",
            "Pose": {
                "type": "compound",
                "values": {                    
                    "#applyFrom": {
                        "type": "float array",
                        "length": 3
                    },
                    "#applyTo": [
                        "Head",
                        "Body",
                        "LeftArm",
                        "RightArm",
                        "LeftLeg",
                        "RightLeg"
                    ]
                }
            },
            "Invisible": "boolean"
        },
        "zombie_pigman": {
            "parent": "living_entity",
            "Anger": "int"
        },
        "villager": {
            "parent": "living_entity",
            "Profession": {
                "type": "byte",
                "values": [0, 1, 2, 3, 4]
            },
            "Offers.Recipes": {
                "type": "compound array",
                "format": {
                    "buy": {
                        "type": "compound",
                        "values": {
                            "id": {
                                "type": "string",
                                "values": "#items"
                            },
                            "Count": "byte"
                        }
                    },
                    "sell": {
                        "type": "compound",
                        "values": {
                            "id": {
                                "type": "string",
                                "values": "#items"
                            },
                            "Count": "byte"                            
                        }
                    },
                    "maxUses": "int"
                }
            }
        },
        "horse": {
            "parent": "living_entity",
            "Type": "int",
            "Saddle": "boolean",
            "Tame": "boolean",
            "Variant": "int"
        },
        "slime": {
            "parent": "living_entity",
            "Size": "int"
        },
        "falling_block": {
            "parent": "entity",
            "Block": {
                "type": "string",
                "values": "#blocks"
            },
            "Time": {
                "type": "byte",
                "values": [-1, 0, 1],
                "description": "1: Block will fall normally, 0: Block will despawn immediately, -1: Will never despawn"
            },
            "DropItem": "boolean"
        },        
        "item": {
            "parent": "entity",
            "id": {
                "type": "string",
                "values": "#items"
            },
            "Damage": "int",
            "Count": "byte", 
            "tag": {
                "type": "compound",
                "values": {
                    "Enchantments": {
                        "type": "compound array",
                        "format": {
                            "id": {
                                "type": "string",
                                "values": "#enchantments"
                            },
                            "lvl": "byte"
                        }
                    },
                    "display": {
                        "type": "compound",
                        "format": {
                            "display": {
                                "type": "string",
                                "tool": "mcfunction.RawJsonGen"
                            },
                            "Lore": "string array"                            
                        }
                    },
                    "AttributeModifiers": {
                        "parent": "#AttributeModifiers"
                    },
                    "Unbreakable": "boolean",
                    "SkullOwner": "string",
                    "HideFlags": {
                        "type": "int",
                        "tool": "mcfunction.HideFlags"
                    },
                    "CanDestroy": {
                        "type": "string array",
                        "values": "#blocks"
                    },
                    "PickupDelay": "int"                
                }
            }   
        }    
    }
}